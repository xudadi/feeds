		if(i == _nCurrIndex){
			page+='<a class="active">' + (i + 1) + '</a>';
		}
		
		<!-- 后两页 -->
		if ((_nCurrIndex + 1) <= (_nPageCount - 1)) { 
			 if (i == (_nCurrIndex + 1)){                               
				 sPageName = _sPageName + "_" + (_nCurrIndex + 1) + "." + _sPageExt;
				 page+='<a class="" href="' + sPageName + '">' + ( i + 1 ) +'</a>';
